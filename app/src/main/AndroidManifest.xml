<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kmand.v2app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/readyeat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/readyeat"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StartActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".Register2Activity"></activity>
        <activity android:name=".Register_CommitActivity"></activity>
    </application>

</manifest>